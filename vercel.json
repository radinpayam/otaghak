{
    "version": 2,
    "builds": [
      {
        "src": "nuxt.config.ts",
        "use": "@vercel/nuxt"
      },
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }